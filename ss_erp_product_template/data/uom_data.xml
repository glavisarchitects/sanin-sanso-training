<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!--        uom category-->
        <record id="uom_category_data_electricity" model="uom.category">
            <field name="name">電気量</field>
        </record>
        <record id="uom_category_data_base" model="uom.category">
            <field name="name">台</field>
        </record>
        <record id="uom_category_data_sets" model="uom.category">
            <field name="name">式</field>
        </record>
        <record id="uom_category_data_multiple" model="uom.category">
            <field name="name">本</field>
        </record>
        <record id="uom_category_data_sheet" model="uom.category">
            <field name="name">枚</field>
        </record>
        <record id="uom_category_data_box" model="uom.category">
            <field name="name">箱</field>
        </record>
        <record id="uom_category_data_can" model="uom.category">
            <field name="name">缶</field>
        </record>
        <record id="uom_category_data_etc" model="uom.category">
            <field name="name">△</field>
        </record>

        <!--                uom uom-->
        <record id="uom_uom_data_Wh" model="uom.uom">
            <field name="name">Wh</field>
            <field name="category_id" ref="uom_category_data_electricity"/>
            <field name="factor" eval="1.0"/>
            <field name="uom_type">reference</field>
        </record>
        <record id="uom_uom_data_kWh" model="uom.uom">
            <field name="name">kWh</field>
            <field name="category_id" ref="uom_category_data_electricity"/>
            <field name="uom_type">bigger</field>
            <field name="factor_inv">1000</field>
        </record>
        <record id="uom_uom_data_base" model="uom.uom">
            <field name="name">台</field>
            <field name="category_id" ref="uom_category_data_base"/>
            <field name="uom_type">reference</field>
            <field name="factor_inv">1</field>
        </record>
        <record id="uom_uom_data_sets" model="uom.uom">
            <field name="name">式</field>
            <field name="category_id" ref="uom_category_data_sets"/>
            <field name="uom_type">reference</field>
            <field name="factor_inv">1</field>
        </record>
        <record id="uom_uom_data_multiple" model="uom.uom">
            <field name="name">本</field>
            <field name="category_id" ref="uom_category_data_multiple"/>
            <field name="uom_type">reference</field>
            <field name="factor_inv">1</field>
        </record>
        <record id="uom_uom_data_sheet" model="uom.uom">
            <field name="name">枚</field>
            <field name="category_id" ref="uom_category_data_sheet"/>
            <field name="uom_type">reference</field>
            <field name="factor_inv">1</field>
        </record>
        <record id="uom_uom_data_box" model="uom.uom">
            <field name="name">箱</field>
            <field name="category_id" ref="uom_category_data_box"/>
            <field name="uom_type">reference</field>
            <field name="factor_inv">1</field>
        </record>
        <record id="uom_uom_data_can" model="uom.uom">
            <field name="name">缶</field>
            <field name="category_id" ref="uom_category_data_can"/>
            <field name="uom_type">reference</field>
            <field name="factor_inv">1</field>
        </record>
        <record id="uom_uom_data_etc" model="uom.uom">
            <field name="name">△</field>
            <field name="category_id" ref="uom_category_data_etc"/>
            <field name="uom_type">reference</field>
            <field name="factor_inv">1</field>
        </record>
    </data>
</odoo>
